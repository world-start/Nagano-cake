<h4>配送先登録/一覧</h4>

<%= form_with model:@address, url: , local:true do |f| %>
<div class="field">
    <%= f.label :郵便番号 %>
    <%= f.text_field :postal_code %>
  </div>
  
  <div class="field">
    <%= f.label :住所 %>
    <%= f.text_field :address %>
  </div>
  
   <div class="field">
    <%= f.label :宛名 %>
    <%= f.text_field :name %>
  </div>
  
  <% end %>
  
  <table>
      <thead>
          <tr>
             <th>郵便番号</th>
             <th>住所</th>
             <th>宛名</th>
             <th></th>
          </tr>
      </thead>
      <tbody>
          <tr>
             <td></td>
             <td></td>
             <td></td>
             <td></td>
          </tr>
      </tbody>
  </table>